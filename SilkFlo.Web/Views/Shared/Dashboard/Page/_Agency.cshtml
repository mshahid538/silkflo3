@model SilkFlo.Web.ViewModels.Home

@{
    const string h1 = "    <h1 style=\"text-align: center; margin-top:32px; \">{text}</h1>";
    const string underConstruction = "<div style = \"margin-left: auto; " +
                                     "margin-right: auto; " +
                                     "max-width: 300px; \"> " +
                                     "<img src = \"/Images/Land Rover.png\" " +
                                     "style = \"max-width: 300px;\" />" +
                                     "</div>";
    var s = h1 + underConstruction;
    var text = "Agency Dashboard for " + Model.AgencyDashboard?.Title;
    s = s.Replace("{text}", text);
}


<div class="container silkflo_tile_container">
     @await Html.PartialAsync("../Shared/Dashboard/Component/_SilkFlo", s)
</div>