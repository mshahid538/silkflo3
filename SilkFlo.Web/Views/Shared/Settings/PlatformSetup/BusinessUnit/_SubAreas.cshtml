@model List<SilkFlo.Web.Models.Business.Process>

@foreach (var process in Model)
{
    <div class="row" id="@process.Id" name="Business.SubArea.Row" style="width:100%; margin-left: 0px;padding:0px;">
        <div class="col-11" column="SubArea" style="padding:10px 15px"
         name="Business.Process"
         id="@process.Id"
         ondblclick="SilkFlo.ViewModels.Settings.PlatformSetup.BusinessUnits.Dbl_Click(event);"
         onclick="SilkFlo.ViewModels.Settings.PlatformSetup.BusinessUnits.Select_Click ( event );">
            <div name="Business.Process.Name" role="textbox" class="child-div" style="color:#353535;">@process.Name</div>
            <input name="NameOld" type="hidden" value="@process.Name" />
            <input name="Business.Process.Id" type="hidden" value="@process.Id" />
            <input name="Business.Process.TeamId" type="hidden" value="@process.TeamId" />
        </div>
        <div class="col-1" style="padding:10px 5px" id="@process.Id" name="Business.Process">
            <span title="Delete">
                <svg xmlns="http://www.w3.org/2000/svg" id="@process.Id" width="16" height="16" fill="currentColor" class="bi bi-trash child-div" viewBox="0 0 16 16" style="color:#A1A1AE;" onclick="SilkFlo.ViewModels.Settings.PlatformSetup.BusinessUnits.Delete_Click ();">
                    <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5Zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6Z"></path>
                    <path d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1ZM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118ZM2.5 3h11V2h-11v1Z"></path>
                </svg>
            </span>

        </div>
    </div>
    <hr style="margin: 0px;color: #5f5f5f;background-color: #5f5f5f;" />
}
<div name="button" style="color:#A1A1AE; padding:10px 15px;" id="btnNewSubArea" class="listbtn"
     onclick="SilkFlo.ViewModels.Settings.PlatformSetup.BusinessUnits.NewSubArea_Click ();">
    + Add Sub-Area
</div>